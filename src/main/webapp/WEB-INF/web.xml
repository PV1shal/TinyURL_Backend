<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet-mapping>
        <servlet-name>Servlets.redirectServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlets.tinyServlet</servlet-name>
        <url-pattern>/maketiny</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlets.userServlet</servlet-name>
        <url-pattern>/user/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlets.historyServlet</servlet-name>
        <url-pattern>/history/*</url-pattern>
    </servlet-mapping>
</web-app>
